#include <appinfo.rh>
#include <appmgr.rsg>

UID2 KUidAppRegistrationResourceFile
#ifdef EKA2
UID3 0xF0201512 
#else
UID3 0x10201512 
#endif

RESOURCE APP_REGISTRATION_INFO
    {
    app_file = "AppMgr";
    
    localisable_resource_id = R_APPMGR_LOCALISABLE_APP_INFO;
    
    embeddability= KAppNotEmbeddable;
    hidden= KAppNotHidden;
    newfile= KAppSupportsNewFile;

    datatype_list= 
        {
        DATATYPE
            {
            priority=EDataTypePriorityNormal;
            type="x-application/x-python";
            }
        };
    }
